function cn_deployContainer() {

	var conId = document.getElementById('X-Connector-Id');
	var patt=/^[0-9a-zA-Z_]+$/;
	if (!patt.test(conId.value) || conId.value.length > 64){
		showCriticalError("ID syntax error!<br>Valid characters are [0-9a-zA-Z_], the length must be less than 64 characters.");
		return false;
	}
	var fname = document.getElementById('selectedFile').value;
	if (fname == "") {
		showCriticalError("Please select Application Archive file");
		return false;
	} else if (fname.toLowerCase().match(/\.zip$|\.ova$/) == null) {
		showCriticalError("Only file extention with zip or ova are allowed.");
		return false;
	}
	CHttpFormExtractor.submitIoxVmUpload(conId.value, fname, function (rtn) { document.forms["cn_deployContainerForm"].submit(); });
	return false;
}

