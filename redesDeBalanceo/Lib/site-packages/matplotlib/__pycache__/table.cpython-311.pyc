var sysUpTime = 0;
var sysTime = 0;

function showTime() {
	sysUpTime = document.getElementById('sys_uptime').innerHTML;
	sysTime = document.getElementById('sys_stime').innerHTML;
	setInterval(function() { timerCount() }, 1000);
}

function convertSecondToTime(sec) {
	var newTime = new Date(sec*1000);
	var days = Math.floor(sec/86400);
	var output = ("0"+newTime.getUTCHours()).slice(-2) + ":" + ("0"+newTime.getUTCMinutes()).slice(-2) + ":" + ("0"+newTime.getUTCSeconds()).slice(-2);
	if (days > 0)
		return (days + "Day(s) and " + output);
	return output;
}

function convertSecondToDateTime(sec) {
	var newTime = new Date(sec*1000);
	var output = ("0"+(newTime.getUTCMonth()+1)).slice(-2) + "/" + ("0"+(newTime.getUTCDate())).slice(-2) + "/" + newTime.getUTCFullYear() + " " + ("0"+newTime.getUTCHours()).slice(-2) + ":" + ("0"+newTime.getUTCMinutes()).slice(-2) + ":" + ("0"+newTime.getUTCSeconds()).slice(-2) + " UTC(UTC-0:00:00)";
	return output;
}

function timerCount() {
	sysUpTime++;
	sysTime++;
	var uptime = document.getElementById('sys_uptime');
	uptime.innerHTML = convertSecondToTime(sysUpTime);

	var systime = document.getElementById('sys_stime');
	systime.innerHTML = convertSecondToDateTime(sysTime);
}


